generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  uuid       String   @unique @default(uuid())
  user_email String   @unique
  user_pwd   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
} 